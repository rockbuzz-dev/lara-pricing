{
  "name": "rockbuzz/lara-pricing",
  "description": "Pricing management",
  "keywords": ["pricing", "plans", "subscriptions", "features"],
  "license": "MIT",
  "authors": [
    {
      "name": "Tiago Lopes",
      "email": "tiago@rockbuzz.com.br"
    },
    {
      "name": "Paulo Henrique P. Scheuermann",
      "email": "paulo@rockbuzz.com.br"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rockbuzz\\LaraPricing\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.3",
    "bensampo/laravel-enum": "^1.32",
    "rockbuzz/lara-uuid": "^1.0",
    "spatie/laravel-sluggable": "^2.2",
    "spatie/eloquent-sortable": "^3.11"
  },
  "require-dev": {
    "orchestra/testbench": "^5.0",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "scripts": {
    "test": "vendor/bin/phpunit --testdox",
    "cs": "vendor/bin/phpcs"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rockbuzz\\LaraPricing\\ServiceProvider"
      ]
    }
  }
}
